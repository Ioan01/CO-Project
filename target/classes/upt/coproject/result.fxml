<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<BorderPane prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="upt.coproject.ResultController">
   <center>
      <TableView prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
        <columns>
          <TableColumn prefWidth="98.0" text="C1" />
          <TableColumn prefWidth="85.0" text="C2" />
        </columns>
      </TableView>
   </center>
   <bottom>
      <HBox BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="mainPageButton" mnemonicParsing="false" onAction="#goToMainPage" text="Run another benchmark" />
            <Button mnemonicParsing="false" text="Save result to database" />
         </children>
      </HBox>
   </bottom>
   <top>
      <HBox prefHeight="180.0" prefWidth="600.0" BorderPane.alignment="CENTER">
         <children>
            <VBox prefHeight="173.0" prefWidth="437.0" style="-fx-alignment: center;">
               <children>
                  <Label text="Your score is:">
                     <font>
                        <Font size="16.0" />
                     </font>
                  </Label>
                  <Label fx:id="scoreLabel" text="SCORE">
                     <font>
                        <Font size="25.0" />
                     </font>
                  </Label>
                  <Label fx:id="gandacelLabel" text="Furnicuță">
                     <font>
                        <Font size="16.0" />
                     </font>
                  </Label>
               </children>
            </VBox>
            <Region prefHeight="200.0" prefWidth="200.0" HBox.hgrow="ALWAYS" />
            <ImageView fitHeight="173.0" fitWidth="203.0">
               <image>
                  <Image url="@../../img/gandacei/ant.jpg" />
               </image>
            </ImageView>
         </children>
      </HBox>
   </top>
</BorderPane>
