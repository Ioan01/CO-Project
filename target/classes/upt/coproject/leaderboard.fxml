<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>

<BorderPane prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="upt.coproject.DetailedResultController">
   <bottom>
      <HBox BorderPane.alignment="CENTER">
         <children>
            <Button mnemonicParsing="false" onAction="#goToPrevPage" text="Go to previous page" />
            <Button mnemonicParsing="false" onAction="#goToMainPage" text="Go to main page" />
            <Button mnemonicParsing="false" onAction="#saveToDatabase" text="Save result to database" />
         </children>
      </HBox>
   </bottom>
   <center>
      <TableView prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <columns>
            <TableColumn prefWidth="98.0" text="C1" />
            <TableColumn prefWidth="85.0" text="C2" />
         </columns>
      </TableView>
   </center>
</BorderPane>
