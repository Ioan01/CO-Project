<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.effect.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>


<HBox alignment="CENTER_RIGHT" prefHeight="600.0" prefWidth="800.0" style="-fx-background-image: /styles/ram.png;" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="upt.coproject.RAM_Controller">
   <children>
      <VBox alignment="CENTER">
         <children>
            <Label text="RAM Benchmark" textFill="#ff9500">
               <font>
                  <Font name="Bodoni MT Condensed Bold" size="28.0" />
               </font>
               <effect>
                  <Lighting>
                     <bumpInput>
                        <Shadow />
                     </bumpInput>
                     <light>
                        <Light.Distant />
                     </light>
                  </Lighting>
               </effect>
            </Label>
            <Button mnemonicParsing="false" onAction="#start_RAM" text="START">
               <font>
                  <Font name="System Bold" size="16.0" />
               </font>
               <effect>
                  <SepiaTone />
               </effect>
            </Button>
            <Button fx:id="results_ram" mnemonicParsing="false" onAction="#goToResults" text="Check Results">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
            </Button>
            <ProgressBar fx:id="progress_ram" prefWidth="200.0" progress="0.0" />
            <Button mnemonicParsing="false" onAction="#goToMainPage" text="Back">
               <font>
                  <Font name="System Bold" size="16.0" />
               </font>
               <effect>
                     <DropShadow />
               </effect>
            </Button>
         </children>
      </VBox>
   </children>
</HBox>
